{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "react/prop-types": 0,
    "semi": 1,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "allowForLoopAfterthoughts": true,
    "prettier/prettier": [
      "error", {
        "semi": true,
        "tabWidth": 2,
        "printWidth": 120,
        "useTabs": true,
        singleQuote: true,
        quoteProps: consistent
      }
    ]
  }
}